<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="Eccube\Repository\ProductClassRepository" name="Eccube\Entity\ProductClass" table="dtb_product_class" inheritance-type="SINGLE_TABLE">
    <discriminator-column name="discriminator_type" type="string" length="255"/>
    <discriminator-map>
      <discriminator-mapping value="productclass" class="Eccube\Entity\ProductClass"/>
    </discriminator-map>
    <indexes>
      <index name="dtb_product_class_price02_idx" columns="price02"/>
      <index name="dtb_product_class_stock_stock_unlimited_idx" columns="stock,stock_unlimited"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="code" type="string" column="product_code" length="255" nullable="true"/>
    <field name="stock" type="decimal" column="stock" precision="10" scale="0" nullable="true"/>
    <field name="stock_unlimited" type="boolean" column="stock_unlimited" nullable="false">
      <options>
        <option name="default">false</option>
      </options>
    </field>
    <field name="sale_limit" type="decimal" column="sale_limit" precision="10" scale="0" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="price01" type="decimal" column="price01" precision="12" scale="2" nullable="true"/>
    <field name="price02" type="decimal" column="price02" precision="12" scale="2" nullable="false"/>
    <field name="delivery_fee" type="decimal" column="delivery_fee" precision="12" scale="2" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="visible" type="boolean" column="visible" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="create_date" type="datetimetz" column="create_date" nullable="false"/>
    <field name="update_date" type="datetimetz" column="update_date" nullable="false"/>
    <field name="currency_code" type="string" column="currency_code" nullable="true"/>
    <field name="point_rate" type="decimal" column="point_rate" precision="10" scale="0" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <one-to-one field="ProductStock" target-entity="Eccube\Entity\ProductStock" mapped-by="ProductClass" fetch="LAZY">
      <cascade>
        <cascade-remove/>
        <cascade-persist/>
      </cascade>
      <join-columns/>
    </one-to-one>
    <one-to-one field="TaxRule" target-entity="Eccube\Entity\TaxRule" mapped-by="ProductClass" fetch="LAZY">
      <cascade>
        <cascade-remove/>
        <cascade-persist/>
      </cascade>
      <join-columns/>
    </one-to-one>
    <many-to-one field="Product" target-entity="Eccube\Entity\Product" inversed-by="ProductClasses" fetch="LAZY">
      <join-columns>
        <join-column name="product_id" referenced-column-name="id" nullable="true"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="SaleType" target-entity="Eccube\Entity\Master\SaleType" fetch="LAZY">
      <join-columns>
        <join-column name="sale_type_id" referenced-column-name="id" nullable="true"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="ClassCategory1" target-entity="Eccube\Entity\ClassCategory" fetch="LAZY">
      <join-columns>
        <join-column name="class_category_id1" referenced-column-name="id" nullable="true"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="ClassCategory2" target-entity="Eccube\Entity\ClassCategory" fetch="LAZY">
      <join-columns>
        <join-column name="class_category_id2" referenced-column-name="id" nullable="true"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="DeliveryDuration" target-entity="Eccube\Entity\DeliveryDuration" fetch="LAZY">
      <join-columns>
        <join-column name="delivery_duration_id" referenced-column-name="id" nullable="true"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="Creator" target-entity="Eccube\Entity\Member" fetch="LAZY">
      <join-columns>
        <join-column name="creator_id" referenced-column-name="id" nullable="true"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
